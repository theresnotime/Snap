{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-redeclare": "off",
        "no-unused-vars": "off",
        "no-useless-escape": "off"
    },
    "ignorePatterns": [
        "node_modules/",
        "sha512.js"
    ]
}
